<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite verbose="1" name="Dummy Json API" parallel="methods" thread-count="5">

    <test name="Product API tests">
        <classes>
            <class name="com.solvd.carina.dummyjson.apitest.ProductTest">
                <methods>
                    <include name="testCreateProduct"/>
